<div class="container-fluid">
  <h3>Request Beta</h3>

  <div class="row-fluid">

    <%= form_for @email, html: {class: "form-inline invert"} do |f| -%>

      <span class="email-box">e-mail</span><%= f.text_field :emailtext, :class => 'input-small' %>
      <%= f.submit 'Request', :class => 'btn btn-danger right' %>
    <% end -%>
  </div>
</div>

<div class="wrap">
	<%= link_to "View all E-mails", admin_emails_path %>
</div>
